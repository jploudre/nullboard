{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "$": "readonly",
    "SKB": "writable",
    "GistSync": "writable",
    "Storage": "writable",
    "StorageLocal": "writable",
    "LocalStorage": "writable",
    "SyncToGist": "writable"
  },
  "rules": {
    "no-tabs": "off",
    "indent": ["error", 2, { "ignoredNodes": [] }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }]
  }
}
